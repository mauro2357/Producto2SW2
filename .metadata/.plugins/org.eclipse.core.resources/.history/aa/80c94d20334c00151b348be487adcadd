<body>
	<table border="1" cellpadding="auto" cellspacing="auto" width="auto">
				<tr><th>Producto<th><th>Cantidad<th></tr>
				<%	
					ArrayList<Producto> y1 = new ArrayList<Producto>();
					ArrayList<Producto> visitados = new ArrayList<Producto>();
					for(Producto producto: y1){
						if(visitados.contains(producto)) continue;
						int aux=0;
						for(Producto auxproducto: y1){
							if(producto==auxproducto){
								aux++;
								visitados.add(auxproducto);
							}
						}
					} 
				%>
			</table>
	
	</body>
</html>