	$(function() {
			var Entrar = "botones"; 
			$.post("/Meerkat_0.0/ad", {
				entrar : Entrar,
			}, function(responseText) {
				$('#boton1').html(responseText);
			});
			Entrar = "listarpedidoactual";
			$.post("/Meerkat_0.0/mesero", {
				entrar : Entrar,
			}, function(responseText) {
				$('#table1').html(responseText);
			});
		});
	
</script>
