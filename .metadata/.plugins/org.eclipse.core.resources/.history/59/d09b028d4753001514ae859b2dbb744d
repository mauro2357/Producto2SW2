<title>Insert title here</title>
<script type="text/javascript">
	
	function ir_admin(){
		var Entrar = "nada";
		$.post("/Meerkat_0.0/administrador", {
			entrar : Entrar,
		}, window.location.replace("index.jsp"));
	}
	
	function consultar_insumos(){
		alert("ingreso al metodo de jquery");
		var Entrar = "consultar_inventario";
		$.post("/Meerkat_0.0/administrador", {
			
			entrar : Entrar,
		}, function(responseText) {
				$('#cuerpo').html(responseText);
			});			
			
		};
	function ir_administrador(){
		var Entrar = "ir_administrador";
		$.post("/Meerkat_0.0/administrador", {
			entrar : Entrar,
		}, window.location.replace("consultarinventariovista/funcionesadministrador.jsp"));
	}
		function insultar(){
		var Entrar = "mierda";
		$.post("/Meerkat_0.0/administrador");
	}
			
</script>
</head>
<body>
	<button type="button" onclick="ir_administrador()">Administrador</button>
	<button type="button" onclick="insultar()">Consultar Inventario</button>



<div id="myDiv"></div>
